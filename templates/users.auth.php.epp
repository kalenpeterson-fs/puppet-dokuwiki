<%- |
      Array $conf_users
| -%>
# users.auth.php
# <?php exit()?>
# Don't modify the lines above
#
# Userfile
#
# Format:
#
# login:passwordhash:Real Name:email:groups,comma,seperated
#
# DO NOT EDIT THIS FILE!
# It is managed by puppet and will be overwritten

<% $conf_users.each |$user| { -%>
<%= $user[login] %>:<%= $user[passwordhash] %>:<%= $user[realname] %>:<%= $user[email] %>:<%= join($user[groups],',') %>
<% } -%>

